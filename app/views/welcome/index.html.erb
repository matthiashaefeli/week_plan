<% if current_user %>
<h2>Welcome <%= current_user.user_name %></h2>
<% end %>
<h2>My Favorite Meals</h2>
<div id="catMeals">
  <% current_user.meals.each do |m| %>
    <% meal = JSON.parse(m.recipe) %>
    <% recipe = meal['meals'][0] %>
    <% like = Like.find_by(meal_id: m.id) %>
    <%= render 'favorite_meals', recipe: recipe, like: like %>
  <% end %>
</div>